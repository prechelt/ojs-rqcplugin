msgid "plugins.generic.reviewqualitycollector.displayName"
msgstr "Review Quality Collector (RQC)"

msgid "plugins.generic.reviewqualitycollector.description"
msgstr "The RQC plugin allows sending review data to the reviewqualitycollector.org service."

msgid "plugins.generic.reviewqualitycollector.settings.description"
msgstr "<p>Review Quality Collector (RQC) is an initiative for improving the quality of scientific peer review. Its core is a mechanism that supplies a reviewer with a receipt for their work for each journal year. The receipt certifies the quality of the rewiewing work based on a journal-specific review quality definition. This turns reviewing into a source of reputation and so improves reviewer motivation and hence review quality.<br> See <a href=\"https://reviewqualitycollector.org/\">reviewqualitycollector.org</a> for details.</p>"

msgid "plugins.generic.reviewqualitycollector.settings.header"
msgstr "RQC configuration for the current journal"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalID"
msgstr "RQC journal ID"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalIDDescription"
msgstr "The journal ID as generated by RQC (you may need to ask your publisher to create one at reviewqualitycollector.org):"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalIDInvalid"
msgstr "RQC journal ID must be an integer"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalKey"
msgstr "RQC secret journal key"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalKeyDescription"
msgstr "The journal key as generated by RQC (you may need to ask your publisher to tell you yours):"

msgid "plugins.generic.reviewqualitycollector.settingsform.rqcJournalKeyInvalid"
msgstr "Invalid RQC journal key. Retrieve the correct one at reviewqualitycollector.org"

msgid "plugins.generic.reviewqualitycollector.editoraction.grade.button"
msgstr "RQC-Grade the Reviews"

msgid "grid.action.rqcGradeName"
msgstr "Send to Review Quality Collector"

msgid "plugins.generic.reviewqualitycollector.editoraction.grade.title"
msgstr "Grade the reviews in RQC"
