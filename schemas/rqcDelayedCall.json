{
	"title": "Delayed RQC call",
	"description": "A call that has to be retried",
	"type": "object",
	"required": [
		"lastTryTS",
		"originalTryTS",
		"remaining_retries",
		"submissionId"
	],
	"properties": {
		"id": {
			"type": "integer",
			"readOnly": true
		},
		"lastTryTS": {
			"type": "integer",
			"validation": [
				"date|Y-m-d H:i:s"
			]
		},
		"originalTryTS": {
			"type": "integer",
			"validation": [
				"date|Y-m-d H:i:s"
			]
		},
		"remaining_retries": {
			"type": "integer",
			"validation": [
				"min:0"
			]
		},
		"submissionId": {
			"type": "integer"
		}
	}
}
